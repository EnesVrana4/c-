@model Dojo
<form action="/Add" method="get">
    <h1>Submitted Info</h1>
    <label>Your Name:</label>
    <label>@Model.YourName</label><br>
    <label>Dojo Location:</label>
    <label>@Model.DojoLocation</label><br>
    <label>Favourite Language:</label>
    <label>@Model.FavouriteLanguage</label><br>
    <label>Comment:</label>
    <label>@Model.Comment</label><br>
    <button type="submit">Go Back</button>
</form>